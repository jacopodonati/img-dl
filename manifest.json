{
    "manifest_version": 2,
    "author": "Jacopo Donati",
    "name": "img-dl",
    "version": "1.0",
    "description": "A simple extension to download strips and backgrounds from selected websites.",
    "content_scripts": [
        {
            "matches": [
                "*://*.bing.com/*"
            ],
            "js": [
                "lib/jquery-3.4.1.min.js",
                "main.js"
            ]
        }
    ]
}